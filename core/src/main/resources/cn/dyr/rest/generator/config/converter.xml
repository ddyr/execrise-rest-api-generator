<?xml version="1.0" encoding="UTF-8" ?>
<converter-config>
    <converters>
        <convert-schema-class>cn.dyr.rest.generator.converter.schema.SSHConvertSchemaImpl</convert-schema-class>
    </converters>

    <packages>
        <po-package>po</po-package>
        <dao-package>dao</dao-package>
        <service-interface-package>service</service-interface-package>
        <service-impl-package>service.impl</service-impl-package>
        <controller-package>controller</controller-package>
        <resource-class-package>hateoas.resource</resource-class-package>
        <resource-assembler-package>hateoas.assembler</resource-assembler-package>
        <common-package>common</common-package>
        <exception-package>exception</exception-package>
    </packages>

    <parameters>
        <builder-style-setter>false</builder-style-setter>
        <paging-enabled>true</paging-enabled>
        <table-prefix>t_</table-prefix>
        <uri-prefix>/v1</uri-prefix>
        <default-page-size>5</default-page-size>
        <cors-enabled>true</cors-enabled>
    </parameters>
</converter-config>